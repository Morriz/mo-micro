{
  "ignore": [
    "build/**/*.js"
  ],
  "presets": [
    [
      "latest-node6", {
        "object-rest": true,
        "es2015": {"loose": true},
        "es2016": false,
        "es2017": false
      }
    ]
  ],
  "plugins": [
    "syntax-async-functions",
    "transform-async-to-generator"
  ]
}